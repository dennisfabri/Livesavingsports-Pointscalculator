<?xml version="1.0" encoding="utf-8" ?>
<CarouselPage
    xmlns:forms="using:Xamarin.Forms" xmlns="http://xamarin.com/schemas/2014/forms"
    x:Name="CP"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:de.df.points.View"
    ItemsSource="{Binding Path=Items}"
    SelectedItem="{Binding Path=Current}"
    Title="{Binding Path=SelectedItem.Title, Source={x:Reference CP}}"
    x:Class="de.df.points.View.AgegroupsPage">
    <CarouselPage.ItemTemplate>
        <DataTemplate>
            <local:AgegroupPage/>
        </DataTemplate>
    </CarouselPage.ItemTemplate>
</CarouselPage>